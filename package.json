{
  "name": "browser-style-dictionary",
  "version": "3.0.2",
  "description": "Style once, use everywhere. A build system for creating cross-platform styles.",
  "keywords": [
    "style dictionary",
    "style",
    "dictionary",
    "amazon",
    "css",
    "design",
    "properties",
    "tokens",
    "sass",
    "scss",
    "iOS",
    "Android",
    "react",
    "react native",
    "style guide"
  ],
  "main": "browser.js",
  "types": "types/index.d.ts",
  "bin": {
    "style-dictionary": "./bin/style-dictionary"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "bin",
    "lib",
    "examples",
    "index.js",
    "browser.js",
    "LICENSE",
    "NOTICE",
    "types"
  ],
  "scripts": {
    "dev": "npm run build && concurrently \"node ./playground/run-browserify.js --watch\" \"rollup -c playground/rollup.config.js --watch\" \"webpack --config playground/webpack.config.js --watch\" \"wds --watch --root-dir dist\"",
    "build": "rimraf dist && node ./playground/run-browserify.js && rollup -c playground/rollup.config.js && webpack --config playground/webpack.config.js",
    "lint": "eslint --fix index.js \"lib/**/*.js\" \"__tests__/**/*.js\"",
    "test": "npm run test-types && npm run lint && jest --runInBand --silent --coverage",
    "test-types": "tsd",
    "test-watch": "npm run test-types && npm run lint && jest --runInBand --watch",
    "generate-docs": "node ./scripts/generateDocs.js",
    "serve-docs": "docsify serve docs -p 3000 -P 12345",
    "install-cli": "npm install -g $(npm pack)",
    "release": "git add . && standard-version -a"
  },
  "standard-version": {
    "scripts": {
      "postbump": "node ./scripts/version.js && npm run generate-docs"
    }
  },
  "jest": {
    "verbose": false,
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "coverageReporters": [
      "html"
    ],
    "coverageDirectory": "docs/coverage",
    "roots": [
      "__tests__",
      "__integration__"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/__setup.js"
    ],
    "testPathIgnorePatterns": [
      "/__tests__/__setup.js",
      "/__tests__/__helpers.js",
      "/__tests__/__assets/",
      "/__tests__/__configs/",
      "/__tests__/__json_files/",
      "/__tests__/__properties/",
      "/__tests__/__output/",
      "/__tests__/formats/__constants.js"
    ],
    "transform": {
      "^.+\\.json5$": "json5-jest",
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/amzn/style-dictionary.git"
  },
  "author": "Amazon",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/amzn/style-dictionary/issues"
  },
  "contributors": [
    "Yuliya Baran <baranyu@amazon.com>",
    "Charles Dorner <dornerc@amazon.com>",
    "Dustin Boudreau <boudrd@amazon.com>",
    "Danny Banks <djb@amazon.com>"
  ],
  "homepage": "https://github.com/amzn/style-dictionary",
  "dependencies": {
    "chalk": "^4.0.0",
    "change-case": "^4.1.2",
    "commander": "^5.1.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.2.0",
    "json5": "^2.1.3",
    "lodash": "^4.17.15",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.2",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@web/dev-server": "^0.1.23",
    "babel-jest": "^26.0.1",
    "babel-preset-env": "^1.7.0",
    "browserify": "^17.0.0",
    "browserify-fs": "github:jorenbroekema/browserify-fs#unique-id",
    "buffer": "^6.0.3",
    "concurrently": "^6.2.2",
    "css-loader": "^6.3.0",
    "docsify": "^4.12.1",
    "docsify-cli": "^4.4.3",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^24.1.3",
    "file-loader": "^6.2.0",
    "husky": "^4.2.5",
    "istanbul": "^0.4.5",
    "jest": "^26.6.3",
    "jsdoc-escape-at": "^1.0.1",
    "jsdoc-to-markdown": "^7.0.1",
    "json5-jest": "^1.0.1",
    "less": "^3.11.2",
    "lint-staged": "^10.2.7",
    "memfs": "^3.3.0",
    "monaco-editor": "^0.28.1",
    "monaco-editor-webpack-plugin": "^4.2.0",
    "node-sass": "^6.0.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "rollup": "^2.57.0",
    "rollup-plugin-copy": "^3.4.0",
    "standard-version": "^9.0.0",
    "style-loader": "^3.3.0",
    "stylus": "^0.54.8",
    "tsd": "^0.15.1",
    "watchify": "^4.0.0",
    "webpack": "^5.55.1",
    "webpack-cli": "^4.8.0",
    "yaml": "^1.10.0",
    "yargs": "^17.2.1"
  }
}
